{
  "name": "mpesa-mz-nodejs-lib",
  "version": "0.6.4",
  "description": "A node.js library for the M-Pesa Mozambique API",
  "main": "index.min.js",
  "scripts": {
    "test": "npx mocha",
    "build": "npx uglifyjs src/transaction.js --compress --mangle --comments --source-map -o index.js",
    "precommit": "npm run build && git add .",
    "commit": "git commit",
    "postcommit": "npm version patch",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose -d docs && start ./docs/index.html",
    "docs:mac": "npx esdoc && open ./docs/index.html",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanruby/mpesa-mz-nodejs-lib.git"
  },
  "keywords": [
    "node",
    "payment",
    "e-commerce",
    "mpesa",
    "mozambique",
    "api"
  ],
  "author": "Ivan Ruby (https://ivanruby.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivanruby/mpesa-mz-nodejs-lib/issues"
  },
  "homepage": "https://github.com/ivanruby/mpesa-mz-nodejs-lib#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "node-rsa": "^1.0.8"
  },
  "devDependencies": {
    "clean-jsdoc-theme": "^2.2.11",
    "dotenv": "^8.2.0",
    "jsdoc": "^3.6.5",
    "mocha": "^8.1.1",
    "npm-version": "^1.1.0",
    "uglify-es": "^3.3.9"
  }
}
